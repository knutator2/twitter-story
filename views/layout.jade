doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(name='description', content='')
    meta(name='author', content='')
    title SB Admin 2 - Bootstrap Admin Theme
    // Bootstrap Core CSS
    link(href='/bower_components/bootstrap/dist/css/bootstrap.min.css', rel='stylesheet')
    // MetisMenu CSS
    link(href='/bower_components/metisMenu/dist/metisMenu.min.css', rel='stylesheet')
    // Morris Chart CSS
    link(href='/bower_components/morrisjs/morris.css', rel='stylesheet')
    // Chartist CSS
    link(href='/bower_components/chartist/dist/chartist.min.css', rel='stylesheet')
    // Chartist Tooltip CSS
    link(href='/bower_components/chartist-plugin-tooltip/dist/chartist-plugin-tooltip.css')
    // Custom CSS
    link(href='/stylesheets/sb-admin-2.css', rel='stylesheet')
    // Custom Fonts
    link(href='/bower_components/font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    // Angular directives
    link(href='/bower_components/angularjs-slider/dist/rzslider.css', rel='stylesheet', type='text/css')
    //Custom
    link(href='/stylesheets/style.css', rel='stylesheet', type='text/css')
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
  body(ng-app='App')
    block nav
    script(src='/bower_components/jquery/dist/jquery.min.js')
    script(src='/bower_components/d3/d3.min.js')
    // Bootstrap Core JavaScript
    script(src='/bower_components/bootstrap/dist/js/bootstrap.min.js')
    // Metis Menu Plugin JavaScript
    script(src='/bower_components/metisMenu/dist/metisMenu.min.js')
    //Morris Charts
    script(src='/bower_components/raphael/raphael.min.js')
    //script(src='/bower_components/morrisjs/morris.min.js')
    //script(src='/javascripts/data/morris-data.js')
    script(src='/bower_components/chartist/dist/chartist.min.js')
    script(src='/bower_components/chartist-plugin-tooltip/dist/chartist-plugin-tooltip.min.js')
    // Custom Theme JavaScript
    script(src='/javascripts/sb-admin-2.js')
    script(src='/bower_components/moment/min/moment.min.js')
    //Linkurious
    script(src='/javascripts/linkurious/dist/sigma.min.js')
    script(src='/javascripts/linkurious/dist/plugins.min.js')

    // Angular
    script(src='/bower_components/angular/angular.min.js')
    // Angular directives
    script(src='/bower_components/chartistAngularDirective.js/build/chartistAngularDirective.min.js')
    script(src='/bower_components/angularjs-slider/dist/rzslider.min.js')

    // Controller
    script(src='/javascripts/app.js')
    script(src='/javascripts/controllers/capture.js')
    script(src='/javascripts/controllers/analyze-overview.js')
    script(src='/javascripts/controllers/network-analysis.js')
    script(src='/javascripts/controllers/modeling.js')


